{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "データ ファクトリ名",
			"defaultValue": "labs-adf-20250701"
		},
		"AzureBlobStorage1_properties_typeProperties_sep": {
			"type": "string",
			"defaultValue": "https://labsstorageaccountprod.blob.core.windows.net/"
		},
		"AzureBlobStorage2_properties_typeProperties_sep": {
			"type": "string",
			"defaultValue": "https://labsstorageaccountpdev.blob.core.windows.net/"
		},
		"credential_dev_properties_typeProperties_rid": {
			"type": "string"
		},
		"credential_prod_properties_typeProperties_rid": {
			"type": "string"
		},
		"credential_stg_properties_typeProperties_rid": {
			"type": "string"
		},
		"containerUri": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを含むストレージ アカウントの URI"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを格納するストレージ アカウントへの SAS トークン"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "labs-adf-20250701_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"credential_dev_properties_typeProperties_rid": {
						"value": "[parameters('credential_dev_properties_typeProperties_rid')]"
					},
					"credential_prod_properties_typeProperties_rid": {
						"value": "[parameters('credential_prod_properties_typeProperties_rid')]"
					},
					"credential_stg_properties_typeProperties_rid": {
						"value": "[parameters('credential_stg_properties_typeProperties_rid')]"
					},
					"AzureBlobStorage1_properties_typeProperties_sep": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_sep')]"
					},
					"AzureBlobStorage2_properties_typeProperties_sep": {
						"value": "[parameters('AzureBlobStorage2_properties_typeProperties_sep')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}