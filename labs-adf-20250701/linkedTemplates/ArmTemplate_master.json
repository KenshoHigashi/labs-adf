{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "データ ファクトリ名",
			"defaultValue": "labs-adf-20250701"
		},
		"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://labsstorageaccountprod.blob.core.windows.net/"
		},
		"AzureBlobStorage2_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://labsstorageaccountpdev.blob.core.windows.net/"
		},
		"credential_dev_properties_typeProperties_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/1e59e522-0988-4b3e-9d49-ba7db35962dd/resourceGroups/labs/providers/Microsoft.ManagedIdentity/userAssignedIdentities/labs_id_dev"
		},
		"credential_prod_properties_typeProperties_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/1e59e522-0988-4b3e-9d49-ba7db35962dd/resourcegroups/labs/providers/Microsoft.ManagedIdentity/userAssignedIdentities/labs_id_prod"
		},
		"credential_stg_properties_typeProperties_resourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/1e59e522-0988-4b3e-9d49-ba7db35962dd/resourcegroups/labs/providers/Microsoft.ManagedIdentity/userAssignedIdentities/labs_id_stg"
		},
		"containerUri": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを含むストレージ アカウントの URI"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "リンクされた ARM テンプレートを格納するストレージ アカウントへの SAS トークン"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "labs-adf-20250701_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"credential_dev_properties_typeProperties_resourceId": {
						"value": "[parameters('credential_dev_properties_typeProperties_resourceId')]"
					},
					"credential_prod_properties_typeProperties_resourceId": {
						"value": "[parameters('credential_prod_properties_typeProperties_resourceId')]"
					},
					"credential_stg_properties_typeProperties_resourceId": {
						"value": "[parameters('credential_stg_properties_typeProperties_resourceId')]"
					},
					"AzureBlobStorage2_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStorage2_properties_typeProperties_serviceEndpoint')]"
					},
					"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_serviceEndpoint')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}